class Solution:
    def productExceptSelf(self, nums: list[int]) -> list[int]:
        n = len(nums)
        a = [1] * n

        # Prefix products
        prefix = 1
        for i in range(n):
            a[i] = prefix
            prefix *= nums[i]

        # Suffix products
        suffix = 1
        for i in range(n - 1, -1, -1):
            a[i] *= suffix
            suffix *= nums[i]

        return a
